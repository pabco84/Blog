<h1>Tienes que ir a posts/dashboard, para ingresar datos</h1>

<table class="table">
<tr>
    <th>Titulo</th>
    <th>Created_at</th>
    <th>Image_url</th>
    <th>Contenido</th>
</tr>
<% @posts.each do |post| %>
    <tr>
        <td><%= post.title%></td>
        <td><%= post.created_at%></td>
        <td><%= post.image_url%></td>
        <td><%= post.content %></td>
    </tr>
<% end %>
</table>
